{
  "name": "testtvapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "expo start",
    "postinstall": "patch-package",
    "clean-ios": "rm -rf ios/build ios/Pods ios/Podfile.lock",
    "setup-ios": "npx pod-install",
    "tvos": "expo run:ios --scheme TestTVApp-tvOS --device \"Apple TV\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "expo": "^51.0.21",
    "react": "18.2.0",
    "react-native": "npm:react-native-tvos@0.74.3-0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.77",
    "@react-native/eslint-config": "0.74.77",
    "@react-native/metro-config": "0.74.77",
    "@react-native/typescript-config": "0.74.77",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.19.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
